


<div class="row">
  <div class="col-sm-6">
    <h1>Backstage_articles</h1>
  </div>

  <div class="col-sm-6 text-right">
  <%= link_to new_backstage_article_path, class: 'btn btn-primary' do %>
    Add New Article
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    

            <th>Title</th>
            <th>Content</th>
            <th>Excerpt</th>
            <th>Seo page title</th>
            <th>Seo description</th>
            <th>Seo url</th>
            
          <th></th>
        
      </tr>
    </thead>

    <tbody>
      <% @backstage_articles.each do |backstage_article| %>
        <%= content_tag :tr, id: dom_id(backstage_article), class: dom_class(backstage_article) do %>
          

                      <td><%= backstage_article.title %></td>
                      <td><%= backstage_article.content %></td>
                      <td><%= backstage_article.excerpt %></td>
                      <td><%= backstage_article.seo_page_title %></td>
                      <td><%= backstage_article.seo_description %></td>
                      <td><%= backstage_article.seo_url %></td>
          
          
            <td><%= link_to 'Show', backstage_article %></td>
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
